<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.phn.szupost">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".SocketApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomePage" />
        <activity android:name=".Regist" />
        <activity android:name=".Setting" />
        <activity android:name=".SettingListDetail" />
        <activity android:name=".Systemsetting" />
        <activity android:name=".Chat" />
        <activity android:name=".Chatlist" />
        <activity android:name=".Changename" />

        <service
            android:name=".MessageService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".Receive_listdetail" />
        <activity android:name=".RList" />
        <activity android:name=".SList" />
        <activity android:name=".JListdetail" />
    </application>

</manifest>